@font-face {
  font-family: 'Josefin Slab';
  font-style: normal;
  font-weight: 400;
  src: local('Josefin Slab'), local('JosefinSlab'), url(http://themes.googleusercontent.com/static/fonts/josefinslab/v3/46aYWdgz-1oFX11flmyEfaRDOzjiPcYnFooOUGCOsRk.woff) format('woff');
}

.lpnav {
  font-family: 'Josefin Slab';
  line-height: 37px;

}

.sticky-wrapper{
  

}

#navbar {
  display:none;
}

.is-sticky{
  display:block !important;
}

.lp-logo{
  position:absolute;
  top:0;
  left:6%;
}

.tray{
  top:50%
  left:50%;
  width:161px;
  position:absolute;
  opacity:.55;
}

.logolp {
  width:108px;
  height:145px;
  position: relative;
  right:auto;
  margin:28px;
}


@media only screen and (max-device-width: 480px) {
  .lpnav {
    display:none;
  }

  #navbar {
    display:block !important;
    top:0px;
  }



}